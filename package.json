{
  "name": "nextjs-planetscale-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma --skip-seed",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "now-build": "yarn generate --schema=./db/schema.prisma && next build"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@next-auth/prisma-adapter": "^0.5.4",
    "@prisma/client": "^3.5.0",
    "@tailwindcss/forms": "^0.3.4",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "^12.0.4",
    "next-auth": "4.0.0-beta.6",
    "next-connect": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-query": "^3.32.3",
    "superagent": "^6.1.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} db/seed.ts"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0-beta.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "mockdate": "^3.0.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1"
  }
}
